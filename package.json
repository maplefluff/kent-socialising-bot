{
	"name": "kent-socialising-bot",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "@sapphire",
	"license": "UNLICENSE",
	"type": "module",
	"imports": {
		"#root/*": [
			"./dist/*.js"
		],
		"#lib/*": [
			"./dist/lib/*.js"
		]
	},
	"dependencies": {
		"@prisma/client": "~6.5.0",
		"@sapphire/decorators": "^6.1.1",
		"@sapphire/discord.js-utilities": "7.1.2",
		"@sapphire/duration": "^1.2.0",
		"@sapphire/eslint-plugin-result": "^2.0.4",
		"@sapphire/framework": "^4.8.5",
		"@sapphire/plugin-hmr": "^3.0.2",
		"@sapphire/plugin-logger": "^3.0.7",
		"@sapphire/plugin-subcommands": "^5.1.0",
		"@sapphire/plugin-utilities-store": "^1.0.3",
		"@sapphire/utilities": "^3.18.2",
		"@skyra/env-utilities": "^1.3.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"colorette": "^2.0.20",
		"discord.js": "^14.19.3",
		"lint-staged": "^15.5.2",
		"pretty-quick": "^3.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.6.1",
		"@commitlint/config-conventional": "^18.6.3",
		"@sapphire/cli": "^1.9.3",
		"@sapphire/eslint-config": "^5.0.6",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.1",
		"@types/eslint": "^8.56.12",
		"@types/node": "^20.17.47",
		"@types/ws": "^8.18.1",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.4.0",
		"husky": "^8.0.3",
		"prettier": "^3.5.3",
		"prisma": "~6.5.0",
		"tsc-watch": "^6.2.1",
		"typescript": "~5.4.5"
	},
	"scripts": {
		"sapphire": "sapphire",
		"generate": "sapphire generate",
		"build": "tsc -b src",
		"watch": "tsc -b src -w",
		"start": "node dist/index.js",
		"dev": "npm run build && npm run start",
		"watch:start": "tsc-watch -b src --onSuccess \"npm run start\"",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint src --ext ts --fix",
		"postinstall": "yarn prisma generate"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@4.0.2"
}
